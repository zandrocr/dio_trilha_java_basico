package edu.zandrocr.tipos_variaveis;

public class TiposVariaveis {
	public static void main(String[] args) {
		byte age = 53;
		short year = 1984;
		int quantity = 2;
		long population = 52123655442L; //para o compilador não confundir com o tipo int
		float pi = 3.13f; //para o compilador não confundir com o tipo double
		double salary = 2542.32;
		String name = "Rafa";
		
		name(name, age, year);
	}
	
	public static void name(String name, byte age, short year) {
		System.out.printf("Ola %s, você tem %s anos nascida em %s", name, age, year);
	}
}