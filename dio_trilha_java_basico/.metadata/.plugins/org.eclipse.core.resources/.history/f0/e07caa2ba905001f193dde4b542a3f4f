package edu.zandrocr.tipos_variaveis;

public class OperadoresTernarios {
	public static void main(String[] args) {
		double sum = 10.5 + 23.3;
		int sub = 32 - 123;
		int mult = 3 * 2;
		int div = 452 / 23;
		int mod = 29 % 2;
		double result = (10 * 4) + (42 / 2);
		
		System.out.printf("soma %s, subtração %s, multiplicação $s, divisão %s, modulo %s, resultado %s", sum, sub, mult, div, mod, result);
	}

}
