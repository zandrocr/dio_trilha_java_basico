package edu.zandrocr.tipos_variaveis;

public class Unarios {
	public static void main(String[] args) {
		int num = 5;
		
		logic(num);
	}
	
	public static void logic(int num) {
		if(num >= 5) {
			System.out.println(++num);
		}else {
			System.out.println("menos");
			
		}
		if(num <= 5) {
			System.out.println(++num);
		}
	}
}
